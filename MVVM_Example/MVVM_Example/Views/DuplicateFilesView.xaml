<Window x:Class="MVVM_Example.Views.DuplicateFilesView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DuplicateFilesView" Height="380" Width="680">
    <Grid>
        <ListBox Name="listFiles" Margin="15,10,26,46" ItemsSource="{Binding DuplicateFiles}"/>
        <TextBlock HorizontalAlignment="Left" Margin="15,309,0,0" TextWrapping="Wrap" Text="{Binding TotalCount}" VerticalAlignment="Top" Width="431" Height="31"/>
    </Grid>
</Window>
